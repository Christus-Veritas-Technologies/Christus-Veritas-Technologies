# Database
# PostgreSQL connection string for Prisma
DATABASE_URL="postgresql://user:password@localhost:5432/christusveritas?schema=public"

# JWT Configuration
# Secret key for signing JWT tokens - change this in production!
JWT_SECRET="your-super-secret-jwt-key-change-this-in-production"

# JWT expiration times
JWT_EXPIRES_IN="7d"
JWT_REFRESH_EXPIRES_IN="30d"

# Server Configuration
# Port the NestJS API server runs on
PORT=3001

# CORS origin for frontend requests
CORS_ORIGIN="http://localhost:3000"

# Node environment
NODE_ENV="development"
